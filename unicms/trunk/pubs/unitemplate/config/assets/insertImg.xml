<?xml version="1.0" encoding="UTF-8"?>
  <insert xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
  <param name="data"/>
  <param name="type"/>
  <param name="width"/>
  <param name="height"/>
  <param name="caption"/>
  <param name="link"/>
  <param name="title"/>
  <template>
    <xhtml:object data="{$data}" type="{$type}" href="{$link}" title="{$title}"><xsl:value-of select="$caption"/></xhtml:object>
  </template>
  </insert>
